#include "main.h"
/**
 * _strchr - find the character
 * @s: string
 * @c: character
 * Return: c
 */

char *_strchr(char *s, char c)
{
	int i = 0;
	int j = 0;
	char *r = ""

	while (s[i] != '\0')
	{
		if (s[i] == c)
		{
			r[j] = s[i];
			j++;
		}
		else
		{
			r = NULL;
		}

		i++;
	}

	return (r);
}
